    <Button
        android:id="@+id/C2R"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="5dp"
        android:background="@android:color/holo_orange_dark"
        android:onClick="add"
        android:text="C2"
        app:layout_constraintBottom_toTopOf="@+id/C3R"
