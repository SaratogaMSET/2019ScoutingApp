        android:text="C2"
        app:layout_constraintStart_toStartOf="@+id/C3L"
        app:layout_constraintTop_toBottomOf="@+id/C3L" />

    <Button
        android:id="@+id/C1L"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/holo_orange_dark"
        android:onClick="add"
        android:text="C1"
        app:layout_constraintStart_toStartOf="@+id/C2L"
