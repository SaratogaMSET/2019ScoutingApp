<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ranas.a2019scoutingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mset_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mset_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SandstormActivity"
            android:label="@string/title_activity_sandstorm" />
        <activity
            android:name=".TeleOpActivity"
            android:label="@string/title_activity_tele_op" />
        <activity
            android:name=".NotesActivity"
            android:label="@string/title_activity_notes"></activity>
    </application>

</manifest>